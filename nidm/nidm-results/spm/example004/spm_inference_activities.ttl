    prov:wasAssociatedWith niiri:software_id .

niiri:conjunction_id_2 a prov:Activity , spm:KConjunctionInference ;
	rdfs:label "k-Conjunction Inference" ;
	nidm_hasAlternativeHypothesis: nidm_OneTailedTest: ;
